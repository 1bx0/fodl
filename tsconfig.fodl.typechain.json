{
    "compilerOptions": {
      "rootDir": "./typechain",
      "outDir": "./typechain",
      "target": "es2018",
      "module": "commonjs",
      "declaration": true,
      "strict": true,
      "esModuleInterop": true,
      "resolveJsonModule": true,
      "noImplicitAny": false,
    },
    "include": ["./typechain"],
    "exclude": ["./bot/**/*", "./scripts/**/*", "./test/**/*", ".deploy/**/*", "./deployments/**/*"]
}