{
  "name": "hardhat-project",
  "license": "MIT",
  "devDependencies": {
    "@atixlabs/hardhat-time-n-mine": "0.0.5",
    "@compound-finance/compound-js": "^0.2.13",
    "@ethereumjs/tx": "3.4.0",
    "@ethereumjs/vm": "5.6.0",
    "@ethersproject/abstract-provider": "^5.4.0",
    "@ethersproject/experimental": "^5.4.0",
    "@nomiclabs/hardhat-ethers": "npm:hardhat-deploy-ethers@^0.3.0-beta.10",
    "@nomiclabs/hardhat-etherscan": "^2.1.6",
    "@nomiclabs/hardhat-waffle": "^2.0.1",
    "@openzeppelin/contracts": "^3.4.2",
    "@openzeppelin/contracts-upgradeable": "^3.4.2",
    "@openzeppelin/hardhat-upgrades": "^1.8.2",
    "@sushiswap/sdk": "^4.1.1",
    "@typechain/ethers-v5": "^7.0.1",
    "@typechain/hardhat": "^2.2.0",
    "@types/chai": "^4.2.18",
    "@types/lodash": "^4.14.171",
    "@types/mocha": "^8.2.2",
    "@types/node": "^15.6.1",
    "@types/pg": "^8.6.1",
    "@uniswap/sdk": "^3.0.3",
    "@uniswap/sdk-core": "^3.0.1",
    "@uniswap/v3-sdk": "^3.3.1",
    "axios": "^0.21.4",
    "chai": "^4.3.4",
    "dotenv": "^10.0.0",
    "ethereum-waffle": "^3.4.0",
    "ethers": "^5.4.5",
    "fs-extra": "^10.0.0",
    "hardhat": "^2.6.4",
    "hardhat-deploy": "^0.9.0",
    "hardhat-deploy-ethers": "^0.3.0-beta.10",
    "hardhat-gas-reporter": "^1.0.4",
    "hardhat-log-remover": "^2.0.2",
    "hardhat-spdx-license-identifier": "^2.0.3",
    "lodash": "^4.17.21",
    "nodemon": "^2.0.7",
    "pg": "^8.7.1",
    "prettier": "^2.3.1",
    "prettier-plugin-solidity": "^1.0.0-beta.13",
    "prompt-async": "^0.9.9",
    "solhint": "^3.3.6",
    "solidity-coverage": "git+https://github.com/sc-forks/solidity-coverage.git",
    "ts-node": "^10.4.0",
    "typechain": "^5.1.2",
    "typescript": "^4.3.2"
  },
  "scripts": {
    "precompile": "npm run clean",
    "compile": "npx hardhat compile --force && tsc -p tsconfig.fodl.typechain.json",
    "test": "npm run compile && npx hardhat test --no-compile",
    "test:unit": "npm run compile --no-compile",
    "test:integration": "npm run compile && npx hardhat test test-integration/index.test.ts --no-compile --config hardhat.integration.config.ts",
    "test:bsc": "npm run clean && npm run compile && npx hardhat test --config hardhat.bsc.config.ts --no-compile",
    "test:polygon": "npm run clean && npm run compile && npx hardhat test --config hardhat.polygon.config.ts --no-compile",
    "clean": "rm -f cache/solidity-files-cache.json cache/validations.json && rm -rf artifacts",
    "coverage": "hardhat coverage",
    "dev": "nodemon",
    "dev:bsc": "nodemon --config nodemon.bsc.json",
    "dev:polygon": "nodemon --config nodemon.polygon.json",
    "dev:custom": "nodemon --config nodemon.custom.json",
    "prependlicense": "hardhat prepend-spdx-license",
    "bot:start": "ts-node bot/src/app.ts",
    "bot:build": "tsc -p tsconfig.bot.build.json",
    "bot:dockerize": "docker build . -t 0xb.one/stoploss -f docker/bot.Dockerfile",
    "removelogs": "hardhhat remove-logs"
  },
  "engines": {
    "node": "^14.17.0",
    "npm": "^6.14.13"
  }
}
